type Categories {
  category: ID! @unique
  categoryname: String!
}

type Products {
  prod_id: ID! @unique
  title: String!
  actor: String
  price: Float!
  special: Float
  common_prod_id: Int
  category: Categories!
}

type Reorder {
  prod_id: Products! @unique
  date_low: DateTime
  quan_low: Int
  date_reordered: DateTime
  quan_reordered: Int
  date_expected: DateTime
}

type Inventory {
  prod_id: Products! @unique
  quan_in_stock: Int!
  sales: Int!
}
